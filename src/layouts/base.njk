<body x-data="{ navIsOpen: false }" :class="{ 'nav-is-open': navIsOpen }" class="bg-gray-900 index">
    {% include "components/header-gray.njk" %}
    <main id="content" rel="main">
        {{ content | safe }}
    </main>
    <div class="bg"></div>
    {% include "components/footer.njk" %}

    <script src="assets/js/loadComponents.js"></script>
    <script src="assets/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="assets/ajax/libs/gsap/3.4.0/gsap.min.js"></script>
